{
  "name": "iside-hasher",
  "productName": "IsideHasher",
  "version": "0.2.0",
  "description": "A simple file hashing tool. Supports MD4, MD5, SHA1, SHA256, RIPEMD160 and other hash algorithms.",
  "license": "CC0-1.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/guildenstern70/IsideHasher",
  "keywords": [
    "hash",
    "MD4",
    "MD5",
    "SHA1",
    "SHA256",
    "SHA384",
    "SHA512",
    "RIPEMD160",
    "hashing",
    "message digest"
  ],
  "author": "Alessio Saltarin",
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-builder": "^22.14.5"
  },
  "dependencies": {
    "electron-context-menu": "^3.1.1"
  },
  "build": {
    "appId": "net.littlelite.iside.electron",
    "mac": {
      "category": "public.app-category.utilities"
    }
  }
}
